{
  "name": "material-son",
  "version": "1.4.0",
  "main": "index.js",
  "repository": "git@github.com:dsslimshaddy/material-son.git",
  "author": "Slim Shady <dsslimshaddy@gmail.com>",
  "license": "MIT",
  "config": {
    "destcopy": "~/gitRepos/utube/dev/js/material-son"
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "scripts": {
    "copy": "rimraf ~/gitRepos/utube/material-son && mkdir -p ~/gitRepos/utube/material-son/ && cp -R ./src/* ~/gitRepos/utube/material-son/",
    "deploy": "cd docs && yarn build && firebase deploy",
    "build": "npm run build:es2015 && npm run build:es2015modules && npm run build:copy-files && npm run build:umd:dev && npm run build:umd:prod",
    "build:es2015": "cross-env NODE_ENV=production babel ./src --ignore *.spec.js --out-dir ./build",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./build/index.es.js",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "build:umd:dev": "webpack --config scripts/umd.webpack.config.js",
    "build:umd:prod": "cross-env NODE_ENV=production webpack --config scripts/umd.webpack.config.js",
    "build:docs": "babel-node ./scripts/build-api-docs.js",
    "prettier": "find . -name \"*.js\" | grep -v -f .eslintignore | xargs prettier --write --single-quote --trailing-comma all --print-width 100",
    "clean:docs": "rimraf docs/api/*",
    "clean:build": "rimraf build",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\"",
    "prebuild": "npm run clean:build",
    "start": "cd docs && npm start",
    "test": "npm run lint && npm run flow && npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test mocha test/**/*.spec.js src/{,**/}*.spec.js --require babel-register",
    "jest:unit": "cross-env NODE_ENV=test jest test/**/*.spec.js src/{,**/}*.spec.js",
    "test:watch": "yarn test:unit -- -w",
    "test:coverage": "cross-env NODE_ENV=test BABEL_ENV=coverage nyc mocha test/**/*.spec.js src/{,**/}*.spec.js && nyc report -r lcovonly",
    "test:coverage:html": "cross-env NODE_ENV=test BABEL_ENV=coverage nyc mocha test/**/*.spec.js src/{,**/}*.spec.js && nyc report --reporter=html",
    "test:karma": "cross-env NODE_ENV=test karma start test/karma.conf.js --single-run",
    "test:regressions": "webpack --config test/regressions/webpack.config.js && vrtest run --config test/vrtest.config.js --record",
    "argos": "argos upload test/regressions/screenshots/chrome --token $ARGOS_TOKEN || true",
    "flow": "flow --show-all-errors",
    "publish": "npm run build && cd build && npm publish --tag next",
    "semantic-release": "semantic-release pre && npm publish ./build && semantic-release post"
  },
  "peerDependencies": {
    "material-ui": "next"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-closure-elimination": "^1.1.16",
    "babel-plugin-direct-import": "^0.0.1-alpha.1",
    "babel-plugin-flow-react-proptypes": "^3.4.1",
    "babel-plugin-groundskeeper-willie": "^1.3.2",
    "babel-plugin-loop-optimizer": "^1.4.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "condition-circle": "^1.5.0",
    "cross-env": "^5.0.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.49.1",
    "flow-copy-source": "^1.2.0",
    "fs-extra": "^3.0.1",
    "jest": "^20.0.4",
    "material-ui": "next",
    "mocha": "^3.4.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "react-test-renderer": "^15.6.1",
    "semantic-release": "^6.3.6",
    "webpack": "^3.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "global": "^4.3.2",
    "prop-types": "^15.5.10",
    "react-contextmenu": "^2.6.5"
  }
}
